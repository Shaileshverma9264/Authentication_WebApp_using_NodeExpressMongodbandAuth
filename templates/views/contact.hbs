<!DOCTYPE html>
<html>

<head>
    {{>link}}
</head>

<body>

    <div>
        <div class="header-dark">
            <nav class="navbar navbar-dark navbar-expand-md navigation-clean-search">
                <div class="container"><a class="navbar-brand" href="#"><i
                            class="fas fa-american-sign-language-interpreting"></i>Skyler<i
                            class="fas fa-american-sign-language-interpreting"></i></a><button class="navbar-toggler"
                        data-toggle="collapse" data-target="#navcol-1"><span class="sr-only">Toggle
                            navigation</span><span class="navbar-toggler-icon"></span></button>
                    <div class="collapse navbar-collapse" id="navcol-1">
                        <ul class="nav navbar-nav">
                            <li class="nav-item" role="presentation"><a class="nav-link" href="/">Home</a></li>
                            <li class="nav-item" role="presentation"><a class="nav-link" href="/about">About Us</a>
                            </li>
                            <li class="nav-item" role="presentation"><a class="nav-link" href="/service">Services</a>
                            </li>
                            <li class="nav-item" role="presentation"><a class="nav-link" href="/contact">Contact</a>
                            </li>

                        </ul>

                        {{>modelform}}
                        {{>registermodel}}


                    </div>
                </div>
            </nav>
            <!-- Contact Us form start -->
            <section>
                <div>
                    <h1 class="text-center text-capitalize mt-5 mb-5">Contact Us</h1>
                    <hr class="w-25 mx-auto pt-5">
                </div>
                <div class="w-50 mx-auto">
                    <form action="/contact" method="POST">
                        <div class="form-group">
                            <label for="name">Name:</label>
                            <input type="text" class="form-control" placeholder="Enter name" id="name"
                                autocomplete="off" name="name">
                        </div>
                        <div class="form-group">
                            <label for="email">Email address:</label>
                            <input type="email" class="form-control" placeholder="Enter email" id="email"
                                autocomplete="off" name="email">
                        </div>
                        <div class="form-group">
                            <label for="mobile">Mobile:</label>
                            <input type="number" class="form-control" placeholder="Enter mobile" id="mobile"
                                autocomplete="off" name="number">
                        </div>
                        <div class="form-group">
                            <label for="message">Message:</label>
                            <textarea type="message" class="form-control" placeholder="Write message...." id="message"
                                autocomplete="off" name="message"></textarea>
                        </div>
                        <div class="form-group">
                            <label for="pwd">Password:</label>
                            <input type="password" class="form-control" placeholder="Enter password" id="pwd"
                                autocomplete="off" name="password">
                        </div>
                        <div class="form-group">
                            <label for="pwd">ConfirmPassword:</label>
                            <input type="password" class="form-control" placeholder="Enter password" id="pwd"
                                autocomplete="off" name="CPassword">
                        </div>
                        <div class="form-group form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" type="checkbox"> Remember me
                            </label>
                        </div>
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </form>
                </div>
            </section>


        </div>
    </div>



    <!-- footer -->
    <div class="footer">
        <p>@copyrigt skyler.com 2021</p>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.bundle.min.js"></script>
</body>

</html>